{
  "name": "static-testing-template",
  "version": "1.0.0",
  "description": "code alone with https://testingjavascript.com/lessons/javascript-lint-javascript-by-configuring-and-running-eslint",
  "main": "index.js",
  "repository": "git@github.com:cdcasey/static-testing-template.git",
  "author": "Chris Casey <cdcasey@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\""
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.0.5"
  }
}
